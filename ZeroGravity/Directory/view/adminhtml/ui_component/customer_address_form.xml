<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd" component="Magento_Customer/js/form/components/form">

    <fieldset name="general">
        <field name="city" sortOrder="95" formElement="select" component="ZeroGravity_Directory/js/form/element/city">
            <settings>
                <dataType>text</dataType>
                <label translate="true">City</label>
                <visible>true</visible>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="ZeroGravity\Directory\Model\ResourceModel\City\Collection"/>
                    </settings>
                </select>
            </formElements>
        </field>
    </fieldset>
</form>
